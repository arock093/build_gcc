

export JN
export JN='-j 8'

rm -rf llvm34
svn co http://llvm.org/svn/llvm-project/llvm/branches/release_34/ llvm34
cd llvm34
cd tools
svn co http://llvm.org/svn/llvm-project/cfe/branches/release_34/ clang
cd ..
./configure --enable-optimized --disable-doxygen --prefix=/opt/llvm34
make $JN
make install

